{
  "global": {
    "version": "v753",
    "skipGeneratedGatewayClasses": true,
    "skipGeneratedPersistentClasses": true,
    "skipGeneratedFunctionGroups": true
  },
  "rules": {
    "7bit_ascii": {
      "enabled": true,
      "exclude": []
    },
    "breakpoint": {
      "enabled": false,
      "exclude": []
    },
    "class_attribute_names": {
      "enabled": true,
      "exclude": [],
      "ignoreExceptions": true,
      "statics": "^G._.*$",
      "instance": "^M._.*$"
    },
    "cloud_types": {
      "enabled": true,
      "exclude": []
    },
    "definitions_top": {
      "enabled": true,
      "exclude": []
    },
    "empty_statement": {
      "enabled": true,
      "exclude": []
    },
    "exit_or_check": {
      "enabled": true,
      "exclude": []
    },
    "exporting": {
      "enabled": true,
      "exclude": []
    },
    "functional_writing": {
      "enabled": true,
      "exclude": [],
      "ignoreExceptions": true
    },
    "keywords_upper": {
      "enabled": true,
      "exclude": [],
      "ignoreExceptions": true
    },
    "line_length": {
      "enabled": true,
      "exclude": [],
      "length": 120
    },
    "check_variables": {
      "enabled": true,
      "exclude": []
    },
    "line_only_punc": {
      "enabled": true,
      "exclude": [],
      "ignoreExceptions": true
    },
    "local_class_naming": {
      "enabled": true,
      "exclude": [],
      "local": "^LCL_.*$",
      "test": "^LTCL_.*$"
    },
    "local_variable_names": {
      "enabled": true,
      "exclude": [],
      "expectedData": "^L._.*$",
      "expectedConstant": "^LC_.*$",
      "expectedFS": "^<L._.*>$"
    },
    "max_one_statement": {
      "enabled": true,
      "exclude": []
    },
    "method_length": {
      "enabled": true,
      "exclude": [],
      "statements": 200
    },
    "method_parameter_names": {
      "enabled": true,
      "exclude": [],
      "ignoreExceptions": true,
      "importing": "^I._.*$",
      "returning": "^R._.*$",
      "changing": "^C._.*$",
      "exporting": "^E._.*$",
      "ignoreNames": [
        "P_TASK"
      ]
    },
    "mix_returning": {
      "enabled": true,
      "exclude": []
    },
    "nesting": {
      "enabled": true,
      "exclude": [],
      "depth": 5
    },
    "indentation": {
      "enabled": true,
      "exclude": [],
      "ignoreExceptions": true
    },
    "obsolete_statement": {
      "enabled": true,
      "exclude": [],
      "refresh": true,
      "compute": true,
      "add": true,
      "subtract": true,
      "multiply": true,
      "move": true,
      "divide": true,
      "requested": true
    },
    "global_class": {
      "enabled": true,
      "exclude": []
    },
    "inline_data_old_versions": {
      "enabled": true,
      "exclude": []
    },
    "parser_error": {
      "enabled": true,
      "exclude": []
    },
    "colon_missing_space": {
      "enabled": true,
      "exclude": []
    },
    "contains_tab": {
      "enabled": true,
      "exclude": []
    },
    "sequential_blank": {
      "enabled": true,
      "exclude": [],
      "lines": 4
    },
    "space_before_colon": {
      "enabled": true,
      "exclude": []
    },
    "start_at_tab": {
      "enabled": true,
      "exclude": []
    },
    "whitespace_end": {
      "enabled": true,
      "exclude": []
    }
  }
}
